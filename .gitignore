# --------------------------------------
# Laravel Framework 11.x - .gitignore
# --------------------------------------

# Environment
.env
.env.*
!.env.example

# Dependencies
/vendor
/node_modules

# Laravel storage & cache
/storage/*.key
/storage/app/*
!/storage/app/public
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/testing/*
/storage/framework/views/*
/storage/logs/*

# Public hot reload / build files
/public/hot
/public/storage
/public/build

# Compiled files
/bootstrap/cache/*.php
/bootstrap/cache/*.json

# IDE / Editor files
/.idea
/.vscode
*.sublime-project
*.sublime-workspace
*.komodoproject

# OS files
.DS_Store
Thumbs.db
desktop.ini

# Logs
*.log

# PHPUnit / Pest
.phpunit.result.cache
.phpunit.cache
/.phpunit.cache

# PHP-CS-Fixer
.php-cs-fixer.cache

# PHPStan
/phpstan.neon
/phpstan.neon.dist
/phpstan.neon.local
/phpstan.cache

# Rector
/rector.php
/.rector.cache

# NPM / Yarn / PNPM
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Vite
.vite

# Build output
/dist
/build

# Coverage
/coverage
.phpunit.cache

# Composer
/composer.phar

# Homestead
/Homestead.json
/Homestead.yaml

# Docker
/docker-data
docker-compose.override.yml

# Temporary files
*.tmp
*.temp
*.swp
*.swo
*.bak
*~

# Test artifacts
/tests/_output

# Mac specific
._*
.Spotlight-V100
bkup/*
error_log
public/error_log
public/sitemap.xml
